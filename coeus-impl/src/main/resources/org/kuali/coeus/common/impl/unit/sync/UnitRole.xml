<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="UnitRole" parent="UnitRole-parentBean"/>
    <bean id="UnitRole-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.common.framework.unit.sync.UnitRole"/>
        <property name="objectLabel" value="Unit Role"/>
        <property name="inquiryDefinition">
            <ref bean="UnitRole-inquiryDefinition"/>
        </property>
        <property name="lookupDefinition">
            <ref bean="UnitRole-lookupDefinition"/>
        </property>
        <property name="titleAttribute" value="id"/>
        <property name="primaryKeys">
            <list>
                <value>id</value>
            </list>
        </property>
        <property name="attributes">
            <list>
                <ref bean="UnitRole-id"/>
                <ref bean="UnitRole-namespaceCode"/>
                <ref bean="UnitRole-name"/>
                <ref bean="UnitRole-description"/>
                <ref bean="UnitRole-kimTypeId"/>
                <ref bean="UnitRole-active"/>
                <ref bean="UnitRole-unitHierarchy"/>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->


    <bean id="UnitRole-id" parent="UnitRole-id-parentBean"/>
    <bean id="UnitRole-id-parentBean" abstract="true" parent="RoleBo-id"/>

    <bean id="UnitRole-namespaceCode" parent="UnitRole-namespaceCode-parentBean"/>
    <bean id="UnitRole-namespaceCode-parentBean" abstract="true" parent="RoleBo-namespaceCode"/>

    <bean id="UnitRole-name" parent="UnitRole-name-parentBean"/>
    <bean id="UnitRole-name-parentBean" abstract="true" parent="RoleBo-name"/>

    <bean id="UnitRole-description" parent="UnitRole-description-parentBean"/>
    <bean id="UnitRole-description-parentBean" abstract="true" parent="RoleBo-description"/>

    <bean id="UnitRole-kimTypeId" parent="UnitRole-kimTypeId-parentBean"/>
    <bean id="UnitRole-kimTypeId-parentBean" abstract="true" parent="RoleBo-kimTypeId"/>

    <bean id="UnitRole-active" parent="UnitRole-active-parentBean"/>
    <bean id="UnitRole-active-parentBean" abstract="true" parent="RoleBo-active"/>

    <bean id="UnitRole-unitHierarchy" parent="UnitRole-unitHierarchy-parentBean"/>
    <bean id="UnitRole-unitHierarchy-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
        <property name="name" value="unitHierarchy"/>
        <property name="required" value="false"/>
        <property name="label" value="Unit Hierarchy"/>
        <property name="shortLabel" value="Unit Hierarchy"/>
        <property name="summary" value="Unit Hierarchy"/>
        <property name="description" value="Unit Hierarchy"/>
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="UnitRole-inquiryDefinition" parent="UnitRole-inquiryDefinition-parentBean"/>
    <bean id="UnitRole-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Unit Role"/>
        <property name="inquirableClass" value="org.kuali.coeus.common.framework.unit.sync.UnitRoleInquirable"/>
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Unit Role"/>
                    <property name="numberOfColumns" value="1"/>
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
                            <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
                            <bean parent="FieldDefinition" p:attributeName="name"/>
                            <bean parent="FieldDefinition" p:attributeName="description"/>
                            <bean parent="FieldDefinition" p:attributeName="kimTypeId"/>
                            <bean parent="FieldDefinition" p:attributeName="active"/>
                            <bean parent="FieldDefinition" p:attributeName="unitHierarchy"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>


    <!-- Business Object Lookup Definition -->
    <bean id="UnitRole-lookupDefinition" parent="UnitRole-lookupDefinition-parentBean"/>
    <bean id="UnitRole-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupableID" value="unitRoleLookupable"/>
        <property name="title" value="Unit Role"/>
        <property name="defaultSort">
            <bean parent="SortDefinition"/>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id"/>
                <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
                <bean parent="FieldDefinition" p:attributeName="name"/>
                <bean parent="FieldDefinition" p:attributeName="description"/>
                <bean parent="FieldDefinition" p:attributeName="unitHierarchy"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
                <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
                <bean parent="FieldDefinition" p:attributeName="name"/>
                <bean parent="FieldDefinition" p:attributeName="description"/>
                <bean parent="FieldDefinition" p:attributeName="kimTypeId"/>
                <bean parent="FieldDefinition" p:attributeName="active"/>
                <bean parent="FieldDefinition" p:attributeName="unitHierarchy"/>
            </list>
        </property>
    </bean>

</beans>