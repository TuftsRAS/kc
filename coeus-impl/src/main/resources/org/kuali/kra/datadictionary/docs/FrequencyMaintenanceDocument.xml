<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="FrequencyMaintenanceDocument" parent="FrequencyMaintenanceDocument-parentBean"/>
	<bean id="FrequencyMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.award.paymentreports.Frequency"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="FrequencyMaintenanceDocument-EditFrequency"/>
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>frequencyCode</value>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="true" />
	
		
		
		
		
		
		<property name="documentTypeName" value="FrequencyMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
		
		
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="FrequencyMaintenanceDocument-EditFrequency" parent="FrequencyMaintenanceDocument-EditFrequency-parentBean"/>
	<bean id="FrequencyMaintenanceDocument-EditFrequency-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit Frequency"/>
		<property name="title" value="Edit Frequency"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="frequencyCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="numberOfDays"/>
				<bean parent="MaintainableFieldDefinition" p:name="numberOfMonths"/>
				<bean parent="MaintainableFieldDefinition" p:name="repeatFlag" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="advanceNumberOfDays"/>
				<bean parent="MaintainableFieldDefinition" p:name="advanceNumberOfMonths"/>
				<bean parent="MaintainableFieldDefinition" p:name="active" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
			</list>
		</property>
	</bean>
</beans>
