<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="DataDictionaryFile" parent="DataDictionaryFile-parentBean" />
    <bean id="DataDictionaryFile-parentBean" abstract="true" parent="BusinessObjectEntry"
            p:businessObjectClass="org.kuali.coeus.sys.framework.dd.DataDictionaryFile"
            p:objectLabel="Data Dictionary File">
        <property name="lookupDefinition" >
            <ref bean="DataDictionaryFile-lookupDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="DataDictionaryFile-module" />
                <ref bean="DataDictionaryFile-name" />
                <ref bean="DataDictionaryFile-path" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="DataDictionaryFile-module" parent="DataDictionaryFile-nodule-parentBean" />
    <bean id="DataDictionaryFile-nodule-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="module" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Module" />
        <property name="shortLabel" value="Module" />
        <property name="maxLength" value="75" />
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint">
                <property name="allowWhitespace" value="true"/>
            </bean>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="75"/>
        </property>
        <property name="controlField">
            <bean p:size="75" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Module" />
        <property name="description" value="Module" />
    </bean>

    <bean id="DataDictionaryFile-name" parent="DataDictionaryFile-name-parentBean" />
    <bean id="DataDictionaryFile-name-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="name" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Name" />
        <property name="shortLabel" value="Name" />
        <property name="maxLength" value="75" />
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint">
                <property name="allowWhitespace" value="true"/>
            </bean>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="75"/>
        </property>
        <property name="controlField">
            <bean p:size="75" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Name" />
        <property name="description" value="Name" />
    </bean>

    <bean id="DataDictionaryFile-path" parent="DataDictionaryFile-path-parentBean" />
    <bean id="DataDictionaryFile-path-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="path" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Path" />
        <property name="shortLabel" value="Path" />
        <property name="maxLength" value="250" />
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint">
                <property name="allowWhitespace" value="true"/>
            </bean>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="250"/>
        </property>
        <property name="controlField">
            <bean p:size="250" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Path" />
        <property name="description" value="Path" />
    </bean>

    <bean id="DataDictionaryFile-lookupDefinition" parent="DataDictionaryFile-lookupDefinition-parentBean" />
    <bean id="DataDictionaryFile-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Data Dictionary File" />
        <property name="lookupableID" value="dataDictionaryFileLookupable" />
        <property name="defaultSort" >
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>name</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition" p:attributeName="module"/>
                <bean parent="FieldDefinition" p:attributeName="name"/>
                <bean parent="FieldDefinition" p:attributeName="path"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="module"/>
                <bean parent="FieldDefinition" p:attributeName="name"/>
                <bean parent="FieldDefinition" p:attributeName="path"/>
            </list>
        </property>
    </bean>

</beans>
