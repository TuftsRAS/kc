 <beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="IacucProtocolStatusMaintenanceDocument" parent="IacucProtocolStatusMaintenanceDocument-parentBean"/>
    <bean id="IacucProtocolStatusMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.kra.iacuc.actions.IacucProtocolStatus"/>
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
        <property name="maintainableSections">
            <list>
                <ref bean="IacucProtocolStatusMaintenanceDocument-EditProtocolStatus"/>
            </list>
        </property>
        <property name="lockingKeys">
            <list>
                <value>protocolStatusCode</value>
            </list>
        </property>
        <property name="allowsRecordDeletion" value="true" />        
        <property name="documentTypeName" value="IacucProtocolStatusMaintenanceDocument"/>
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="IacucProtocolStatusMaintenanceDocument-EditProtocolStatus" parent="IacucProtocolStatusMaintenanceDocument-EditProtocolStatus-parentBean"/>
    <bean id="IacucProtocolStatusMaintenanceDocument-EditProtocolStatus-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Protocol Status"/>
        <property name="title" value="Edit Protocol Status"/>
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="protocolStatusCode" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
            </list>
        </property>
    </bean>
</beans>
