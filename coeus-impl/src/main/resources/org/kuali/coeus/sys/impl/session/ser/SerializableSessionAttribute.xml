<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="SerializableSessionAttribute" parent="SerializableSessionAttribute-parentBean" />
    <bean id="SerializableSessionAttribute-parentBean" abstract="true" parent="BusinessObjectEntry"
            p:businessObjectClass="org.kuali.coeus.sys.impl.session.ser.SerializableSessionAttribute"
            p:objectLabel="Serializable Session Attribute">
        <property name="lookupDefinition" >
            <ref bean="SerializableSessionAttribute-lookupDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="SerializableSessionAttribute-sessionId" />
                <ref bean="SerializableSessionAttribute-name" />
                <ref bean="SerializableSessionAttribute-type" />
                <ref bean="SerializableSessionAttribute-value" />
                <ref bean="SerializableSessionAttribute-serializable" />
                <ref bean="SerializableSessionAttribute-size" />
                <ref bean="SerializableSessionAttribute-failurePath" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="SerializableSessionAttribute-sessionId" parent="SerializableSessionAttribute-sessionId-parentBean" />
    <bean id="SerializableSessionAttribute-sessionId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="sessionId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Session Id" />
        <property name="shortLabel" value="Session Id" />
        <property name="maxLength" value="100" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint">
                <property name="allowWhitespace" value="true"/>
            </bean>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="100"/>
        </property>
        <property name="controlField">
            <bean p:size="100" parent="Uif-TextControl"/>
        </property>
        <property name="attributeSecurity">
            <bean parent="AttributeSecurity">
                <property name="partialMask" value="true"/>
                <property name="partialMaskFormatter">
                    <bean parent="MaskFormatterSubString" p:maskLength="27"/>
                </property>
            </bean>
        </property>
        <property name="summary" value="Session Id" />
        <property name="description" value="Session Id" />
    </bean>

    <bean id="SerializableSessionAttribute-name" parent="SerializableSessionAttribute-name-parentBean" />
    <bean id="SerializableSessionAttribute-name-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="name" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Name" />
        <property name="shortLabel" value="Name" />
        <property name="maxLength" value="100" />
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint">
                <property name="allowWhitespace" value="true"/>
            </bean>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="100"/>
        </property>
        <property name="controlField">
            <bean p:size="100" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Name" />
        <property name="description" value="Name" />
    </bean>

    <bean id="SerializableSessionAttribute-type" parent="SerializableSessionAttribute-type-parentBean" />
    <bean id="SerializableSessionAttribute-type-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="type" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Type" />
        <property name="shortLabel" value="Type" />
        <property name="maxLength" value="100" />
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint">
                <property name="allowWhitespace" value="true"/>
            </bean>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="100"/>
        </property>
        <property name="controlField">
            <bean p:size="100" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Type" />
        <property name="description" value="Type" />
    </bean>

    <bean id="SerializableSessionAttribute-value" parent="SerializableSessionAttribute-value-parentBean" />
    <bean id="SerializableSessionAttribute-value-parentBean" abstract="true" parent="AttributeReferenceDummy-extendedTextArea"
          p:name="value">
        <property name="label" value="Value" />
        <property name="shortLabel" value="Value" />
        <property name="summary" value="Value" />
        <property name="description" value="Value" />
    </bean>

    <bean id="SerializableSessionAttribute-serializable" parent="SerializableSessionAttribute-serializable-parentBean" />
    <bean id="SerializableSessionAttribute-serializable-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean"
          p:name="serializable">
        <property name="label" value="Serializable" />
        <property name="shortLabel" value="Serializable" />
        <property name="summary" value="Serializable" />
        <property name="description" value="Serializable" />
    </bean>

    <bean id="SerializableSessionAttribute-size" parent="SerializableSessionAttribute-size-parentBean" />
    <bean id="SerializableSessionAttribute-size-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="size" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Size" />
        <property name="shortLabel" value="Size" />
        <property name="maxLength" value="100" />
        <property name="required" value="true" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern"/>
        </property>
        <property name="validCharactersConstraint">
            <bean parent="NumericPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="100"/>
        </property>
        <property name="controlField">
            <bean p:size="100" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Size" />
        <property name="description" value="Size" />
    </bean>

    <bean id="SerializableSessionAttribute-failurePath" parent="SerializableSessionAttribute-failurePath-parentBean" />
    <bean id="SerializableSessionAttribute-failurePath-parentBean" abstract="true" parent="AttributeReferenceDummy-extendedTextArea"
          p:name="failurePath">
        <property name="label" value="Failure Path" />
        <property name="shortLabel" value="Failure Path" />
        <property name="summary" value="Failure Path" />
        <property name="description" value="Failure Path" />
    </bean>

    <bean id="SerializableSessionAttribute-lookupDefinition" parent="SerializableSessionAttribute-lookupDefinition-parentBean" />
    <bean id="SerializableSessionAttribute-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Serializable Session Attribute" />
        <property name="lookupableID" value="serializableSessionLookupable" />
        <property name="defaultSort" >
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>sessionId</value>
                        <value>name</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition" p:attributeName="serializable"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="sessionId"/>
                <bean parent="FieldDefinition" p:attributeName="name"/>
                <bean parent="FieldDefinition" p:attributeName="type"/>
                <bean parent="FieldDefinition" p:attributeName="value"/>
                <bean parent="FieldDefinition" p:attributeName="serializable"/>
                <bean parent="FieldDefinition" p:attributeName="size"/>
                <bean parent="FieldDefinition" p:attributeName="failurePath"/>
            </list>
        </property>
    </bean>

</beans>
