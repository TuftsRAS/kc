<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="UserAttachedFormsXMLReorder" parent="UserAttachedFormsXMLReorder-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.s2s.UserAttachedFormsXMLReorder" />
        <property name="objectLabel" value="User Attached Forms XML Reorder Lookup" />
        <property name="inquiryDefinition" >
            <ref bean="UserAttachedFormsXMLReorder-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="UserAttachedFormsXMLReorder-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="id" />
        <property name="attributes" >
            <list>
                <ref bean="UserAttachedFormsXMLReorder-id" />
                <ref bean="UserAttachedFormsXMLReorder-nodeToMove" />
                <ref bean="UserAttachedFormsXMLReorder-targetNode" />
                <ref bean="UserAttachedFormsXMLReorder-moveAfter" />
                <ref bean="UserAttachedFormsXMLReorder-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="UserAttachedFormsXMLReorder-id" parent="UserAttachedFormsXMLReorder-id-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-id-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="id" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="id" />
        <property name="shortLabel" value="id" />
        <property name="maxLength" value="12" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AlphaNumericPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="4"/>
        </property>
        <property name="controlField">
            <bean p:size="4" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="id" />
        <property name="description" value="id" />
    </bean>

    <bean id="UserAttachedFormsXMLReorder-versionNumber" parent="UserAttachedFormsXMLReorder-versionNumber-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>

    <bean id="UserAttachedFormsXMLReorder-nodeToMove" parent="UserAttachedFormsXMLReorder-nodeToMove-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-nodeToMove-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="nodeToMove" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Node to move" />
        <property name="shortLabel" value="Node to move" />
        <property name="maxLength" value="60" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="4"/>
        </property>
        <property name="controlField">
            <bean p:size="4" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Node to move" />
        <property name="description" value="Node to move" />
    </bean>

    <bean id="UserAttachedFormsXMLReorder-targetNode" parent="UserAttachedFormsXMLReorder-targetNode-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-targetNode-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="targetNode" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Target Node" />
        <property name="shortLabel" value="Target Node" />
        <property name="maxLength" value="60" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="4"/>
        </property>
        <property name="controlField">
            <bean p:size="4" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Target Node" />
        <property name="description" value="Target Node" />
    </bean>

    <bean id="UserAttachedFormsXMLReorder-moveAfter" parent="UserAttachedFormsXMLReorder-moveAfter-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-moveAfter-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="moveAfter" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Move after the target node" />
        <property name="shortLabel" value="Move after the target node" />
        <property name="maxLength" value="1" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AlphaNumericPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="CheckboxControlDefinition" />
        </property>
        <property name="controlField">
            <bean parent="Uif-CheckboxControl"/>
        </property>
        <property name="summary" value="Move after the target node" />
        <property name="description" value="Move after the target node" />
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="UserAttachedFormsXMLReorder-inquiryDefinition" parent="UserAttachedFormsXMLReorder-inquiryDefinition-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-InquiryView" parent="UserAttachedFormsXMLReorder-InquiryView-parentBean"/>
    <bean id="UserAttachedFormsXMLReorder-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="User Attached Forms XML Reorder" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="User Attached Forms XML Reorder" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
                            <bean parent="FieldDefinition" p:attributeName="nodeToMove"/>
                            <bean parent="FieldDefinition" p:attributeName="targetNode"/>
                            <bean parent="FieldDefinition" p:attributeName="moveAfter"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="UserAttachedFormsXMLReorder-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
        <property name="headerText" value="User Attached Forms XML Reorder"/>
        <property name="dataObjectClassName" value="org.kuali.coeus.propdev.impl.s2s.UserAttachedFormsXMLReorder"/>
        <property name="items">
            <list>
                <bean parent="Uif-Disclosure-GridSection">
                    <property name="headerText" value="User Attached Forms XML Reorder"/>
                    <property name="layoutManager.numberOfColumns" value="2"/>
                    <property name="items">
                        <list>
                            <bean p:propertyName="id" parent="Uif-DataField"/>
                            <bean p:propertyName="nodeToMove" parent="Uif-DataField"/>
                            <bean p:propertyName="targetNode" parent="Uif-DataField"/>
                            <bean p:propertyName="moveAfter" parent="Uif-DataField"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="UserAttachedFormsXMLReorder-lookupDefinition" parent="UserAttachedFormsXMLReorder-lookupDefinition-parentBean" />
    <bean id="UserAttachedFormsXMLReorder-LookupView" parent="UserAttachedFormsXMLReorder-LookupView-parentBean"/>
    <bean id="UserAttachedFormsXMLReorder-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="User Attached Forms XML Reorder Lookup" /><property name="defaultSort" >
        <bean parent="SortDefinition"/>
    </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition" p:attributeName="id"/>
                <bean parent="FieldDefinition" p:attributeName="nodeToMove"/>
                <bean parent="FieldDefinition" p:attributeName="targetNode"/>
                <bean parent="FieldDefinition" p:attributeName="moveAfter"/>
            </list>
        </property>
        <property name="resultFields" >
            <list>
                <bean parent="FieldDefinition" p:attributeName="id"/>
                <bean parent="FieldDefinition" p:attributeName="nodeToMove"/>
                <bean parent="FieldDefinition" p:attributeName="targetNode"/>
                <bean parent="FieldDefinition" p:attributeName="moveAfter"/>
            </list>
        </property>
    </bean>

    <bean id="UserAttachedFormsXMLReorder-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
        <property name="dataObjectClassName" value="org.kuali.coeus.propdev.impl.s2s.UserAttachedFormsXMLReorder"/>
        <property name="headerText" value="User Attached Forms XML Reorder Lookup"/>
        <property name="criteriaFields">
            <list>
                <bean p:propertyName="id" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="nodeToMove" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="targetNode" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="moveAfter" parent="Uif-LookupCriteriaInputField"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean p:propertyName="id" parent="Uif-DataField"/>
                <bean p:propertyName="nodeToMove" parent="Uif-DataField"/>
                <bean p:propertyName="targetNode" parent="Uif-DataField"/>
                <bean p:propertyName="moveAfter" parent="Uif-DataField"/>
            </list>
        </property>
    </bean>

</beans>
