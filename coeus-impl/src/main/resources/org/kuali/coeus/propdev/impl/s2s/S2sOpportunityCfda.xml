<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="S2sOpportunityCfda" parent="S2sOpportunityCfda-parentBean" />
  <bean id="S2sOpportunityCfda-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.s2s.S2sOpportunityCfda" />
    <property name="objectLabel" value="S2s Opportunity CFDA" />

    <property name="attributes" >
      <list>
        <ref bean="S2sOpportunityCfda-proposalNumber" />
        <ref bean="S2sOpportunityCfda-cfdaNumber" />
        <ref bean="S2sOpportunityCfda-cfdaDescription" />
        <ref bean="S2sOpportunityCfda-versionNumber" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="S2sOpportunityCfda-proposalNumber" parent="S2sOpportunityCfda-proposalNumber-parentBean" />

  <bean id="S2sOpportunityCfda-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="proposalNumber" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Proposal Number" />
    <property name="shortLabel" value="Proposal Number" />
    <property name="maxLength" value="8" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint"/>
    </property>
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="controlField">
      <bean parent="Uif-HiddenControl"/>
    </property>
    <property name="summary" value="Proposal Number" />
    <property name="description" value="Proposal Number" />
  </bean>

  <bean id="S2sOpportunityCfda-cfdaNumber" parent="S2sOpportunityCfda-cfdaNumber-parentBean"/>
  <bean id="S2sOpportunityCfda-cfdaNumber-parentBean" abstract="true" parent="CFDA-cfdaNumber"/>

  <bean id="S2sOpportunityCfda-cfdaDescription" parent="S2sOpportunityCfda-cfdaDescription-parentBean"/>
  <bean id="S2sOpportunityCfda-cfdaDescription-parentBean" abstract="true" parent="CFDA-cfdaProgramTitleName">
    <property name="name" value="cfdaDescription" />
  </bean>

  <bean id="S2sOpportunityCfda-opportunity" parent="S2sOpportunityCfda-opportunity-parentBean"/>
  <bean id="S2sOpportunityCfda-opportunity-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="opportunity" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Opportunity" />
    <property name="shortLabel" value="Opportunity" />
    <property name="maxLength" value="40000000" />
    <property name="control" >
      <bean parent="HiddenControlDefinition" />
    </property>
    <property name="controlField">
      <bean parent="Uif-HiddenControl"/>
    </property>
    <property name="summary" value="Opportunity" />
    <property name="description" value="Opportunity" />
  </bean>

  <bean id="S2sOpportunityCfda-closingDate" parent="S2sOpportunityCfda-closingDate-parentBean"/>
  <bean id="S2sOpportunityCfda-closingDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="ClosingDate" />
    <property name="maxLength" value="21" />
    <property name="control" >
      <bean parent="TextControlDefinition" p:datePicker="true" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
    </property>
    <property name="summary" value="&amp;nbsp;" />
    <property name="formatterClass" value="org.kuali.coeus.sys.framework.controller.CalendarTimestampAMPMFormatter" />
    <property name="name" value="closingDate" />
    <property name="label" value="Closing Date" />
    <property name="description" value="Closing Date" />
  </bean>


  <bean id="S2sOpportunityCfda-openingDate" parent="S2sOpportunityCfda-openingDate-parentBean" />

  <bean id="S2sOpportunityCfda-openingDate-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false" />
    <property name="shortLabel" value="OpeningDate" />
    <property name="maxLength" value="21" />
    <property name="control" >
      <bean parent="TextControlDefinition" p:datePicker="true" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
    </property>
    <property name="summary" value="Opening Date" />
    <property name="formatterClass" value="org.kuali.coeus.sys.framework.controller.CalendarTimestampAMPMFormatter" />
    <property name="name" value="openingDate" />
    <property name="label" value="Opening Date" />
    <property name="description" value="Opening Date" />
  </bean>

  <bean id="S2sOpportunityCfda-opportunityId" parent="S2sOpportunityCfda-opportunityId-parentBean"/>
  <bean id="S2sOpportunityCfda-opportunityId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="opportunityId" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Opportunity ID" />
    <property name="shortLabel" value="Opportunity ID" />
    <property name="maxLength" value="50" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="50"/>
    </property>
    <property name="controlField">
      <bean p:size="50" parent="Uif-TextControl"/>
    </property>
    <property name="summary" value="The Opportunity ID for this Proposal." />
    <property name="description" value="A unique identifier associated with each sponsor's funding opportunity announcement. AKA &quot;Funding Opportunity Announcement number&quot; or &quot;FOA number&quot;"/>
  </bean>

  <bean id="S2sOpportunityCfda-opportunityTitle" parent="S2sOpportunityCfda-opportunityTitle-parentBean"/>
  <bean id="S2sOpportunityCfda-opportunityTitle-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="opportunityTitle" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Opportunity Title" />
    <property name="shortLabel" value="Opportunity Title" />
    <property name="maxLength" value="255" />
    <property name="validationPattern" >
      <bean parent="UTF8AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="UTF8AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
    </property>
    <property name="required" value="false" />
    <property name="control" >
      <bean parent="TextareaControlDefinition" p:rows="3" p:cols="80"/>
    </property>
    <property name="controlField">
      <bean p:rows="3" p:cols="80" parent="Uif-TextAreaControl"/>
    </property>
    <property name="summary" value="The Opportunity Title for this Proposal." />
    <property name="description" value="The title of a publicly available document, announcing a federal agency's intentions to award discretionary grants or cooperative agreements, usually as a result of competition for funds.  AKA  Funding opportunity announcements, notices of funding availability, or solicitations."/>
  </bean>

  <bean id="S2sOpportunityCfda-revisionCode" parent="S2sOpportunityCfda-revisionCode-parentBean"/>
  <bean id="S2sOpportunityCfda-revisionCode-parentBean" abstract="true" parent="S2sRevisionType-code">
    <property name="name" value="revisionCode" />
  </bean>

  <bean id="S2sOpportunityCfda-revisionOtherDescription" parent="S2sOpportunityCfda-revisionOtherDescription-parentBean"/>
  <bean id="S2sOpportunityCfda-revisionOtherDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="revisionOtherDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Revision Other Description" />
    <property name="shortLabel" value="Revision Other Description" />
    <property name="maxLength" value="45" />
    <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="45"/>
    </property>
    <property name="controlField">
      <bean p:size="45" parent="Uif-TextControl"/>
    </property>
    <property name="summary" value="Revision Other Description" />
    <property name="description" value="Revision Other Description" />
  </bean>

  <bean id="S2sOpportunityCfda-s2sSubmissionTypeCode" parent="S2sOpportunityCfda-s2sSubmissionTypeCode-parentBean"/>
  <bean id="S2sOpportunityCfda-s2sSubmissionTypeCode-parentBean" abstract="true" parent="S2sSubmissionType-code">
      <property name="name" value="s2sSubmissionTypeCode" />
  </bean>
  <bean id="S2sOpportunityCfda-schemaUrl" parent="S2sOpportunityCfda-schemaUrl-parentBean" />
  <bean id="S2sOpportunityCfda-schemaUrl-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="schemaUrl" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Schema URL" />
    <property name="shortLabel" value="Schema URL" />
    <property name="maxLength" value="300" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint"/>
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="300"/>
    </property>
    <property name="controlField">
      <bean p:size="300" parent="Uif-TextControl"/>
    </property>
    <property name="summary" value="Schema URL" />
    <property name="description" value="Schema URL" />
  </bean>
  
  <bean id="S2sOpportunityCfda-providerCode" parent="S2sOpportunityCfda-providerCode-parentBean" />
  <bean id="S2sOpportunityCfda-providerCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="providerCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="S2S Provider" />
    <property name="shortLabel" value="S2S Provider" />
    <property name="maxLength" value="4" />
    <property name="required" value="true"/>
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" />
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint"/>
    </property>
    <property name="control" >
      <bean parent="SelectControlDefinition" p:businessObjectClass="org.kuali.coeus.propdev.impl.s2s.S2sProvider" p:valuesFinderClass="org.kuali.coeus.sys.framework.keyvalue.ExtendedPersistableBusinessObjectValuesFinder" p:includeKeyInLabel="false" p:includeBlankRow="false" p:keyAttribute="code" p:labelAttribute="description"/>
    </property>
    <property name="optionsFinder">
      <bean class="org.kuali.coeus.sys.framework.keyvalue.ExtendedPersistableBusinessObjectValuesFinder">
        <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.s2s.S2sProvider"/>
        <property name="includeKeyInDescription" value="false"/>
        <property name="includeBlankRow" value="false"/>
        <property name="keyAttributeName" value="code"/>
        <property name="labelAttributeName" value="description"/>
      </bean>
    </property>
    <property name="controlField">
      <bean parent="Uif-DropdownControl"/>
    </property>
    <property name="summary" value="S2S Provider" />
    <property name="description" value="S2S Provider" />
  </bean>

  <bean id="S2sOpportunityCfda-multiProject" parent="S2sOpportunityCfda-multiProject-parentBean" />

  <bean id="S2sOpportunityCfda-multiProject-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
    <property name="name" value="multiProject" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Multiple Project" />
    <property name="shortLabel" value="Multi Project" />
    <property name="summary" value="Is this a multiple project opportunity?" />
    <property name="description" value="Is this a multiple project opportunity?" />
  </bean>
  
  <bean id="S2sOpportunityCfda-versionNumber" parent="S2sOpportunityCfda-versionNumber-parentBean" />
  <bean id="S2sOpportunityCfda-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>
<!-- Business Object Inquiry Definition -->
  <bean id="S2sOpportunityCfda-inquiryDefinition" parent="S2sOpportunityCfda-inquiryDefinition-parentBean" />
  <bean id="S2sOpportunityCfda-InquiryView" parent="S2sOpportunityCfda-InquiryView-parentBean"/>
  <bean id="S2sOpportunityCfda-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="S2s Opportunity" />
    <property name="inquirySections" >
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value="S2s Opportunity" />
          <property name="numberOfColumns" value="1" />
          <property name="inquiryFields" >
            <list>
              <bean parent="FieldDefinition" p:attributeName="proposalNumber" p:forceInquiry="true"/>
              <bean parent="InquiryCollectionDefinition">
                <property name="attributeName" value="S2sOpportunityCfdaCfdas"/>
                <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.s2s.S2sOpportunityCfda"/>
                <property name="numberOfColumns" value="2"/>
                <property name="summaryTitle" value="CFDA"/>
                <property name="summaryFields">
                  <list>
                    <bean parent="FieldDefinition" p:attributeName="cfdaNumber"/>
                  </list>
                </property>
                <property name="inquiryFields">
                  <list>
                    <bean parent="FieldDefinition" p:attributeName="cfdaNumber"/>
                  </list>
                </property>
              </bean>
              <bean parent="FieldDefinition" p:attributeName="closingDate"/>
              <bean parent="FieldDefinition" p:attributeName="competetionId"/>
              <bean parent="FieldDefinition" p:attributeName="instructionUrl"/>
              <bean parent="FieldDefinition" p:attributeName="openingDate"/>
              <bean parent="FieldDefinition" p:attributeName="opportunity"/>
              <bean parent="FieldDefinition" p:attributeName="opportunityId"/>
              <bean parent="FieldDefinition" p:attributeName="opportunityTitle"/>
              <bean parent="FieldDefinition" p:attributeName="revisionCode"/>
              <bean parent="FieldDefinition" p:attributeName="revisionOtherDescription"/>
              <bean parent="FieldDefinition" p:attributeName="s2sSubmissionTypeCode"/>
              <bean parent="FieldDefinition" p:attributeName="schemaUrl"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="S2sOpportunityCfda-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
    <property name="headerText" value="S2s Opportunity"/>
    <property name="dataObjectClassName" value="org.kuali.coeus.propdev.impl.s2s.S2sOpportunityCfda"/>
    <property name="items">
      <list>
        <bean parent="Uif-Disclosure-GridSection">
          <property name="headerText" value="S2s Opportunity"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
              <bean p:propertyName="proposalNumber" parent="Uif-DataField"/>
              <bean parent="Uif-StackedCollectionSection">
                <property name="headerText" value="CFDA"/>
                <property name="layoutManager.lineGroupPrototype.layoutManager.numberOfColumns" value="2"/>
                <property name="propertyName" value="S2sOpportunityCfdaCfdas"/>
                <property name="collectionObjectClass" value="org.kuali.coeus.propdev.impl.s2s.S2sOpportunityCfda"/>
                <property name="items">
                  <list>
                    <bean p:propertyName="cfdaNumber" parent="Uif-DataField"/>
                  </list>
                </property>
                <property name="layoutManager.summaryFields">
                  <list>
                    <value>cfdaNumber</value>
                  </list>
                </property>
              </bean>
              <bean p:propertyName="closingDate" parent="Uif-DataField"/>
              <bean p:propertyName="competetionId" parent="Uif-DataField"/>
              <bean p:propertyName="instructionUrl" parent="Uif-DataField"/>
              <bean p:propertyName="openingDate" parent="Uif-DataField"/>
              <bean p:propertyName="opportunity" parent="Uif-DataField"/>
              <bean p:propertyName="opportunityId" parent="Uif-DataField"/>
              <bean p:propertyName="opportunityTitle" parent="Uif-DataField"/>
              <bean p:propertyName="revisionCode" parent="Uif-DataField"/>
              <bean p:propertyName="revisionOtherDescription" parent="Uif-DataField"/>
              <bean p:propertyName="s2sSubmissionTypeCode" parent="Uif-DataField"/>
              <bean p:propertyName="schemaUrl" parent="Uif-DataField"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

</beans>
