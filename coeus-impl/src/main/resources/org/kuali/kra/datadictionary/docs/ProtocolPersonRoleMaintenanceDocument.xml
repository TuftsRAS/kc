 <beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="ProtocolPersonRoleMaintenanceDocument" parent="ProtocolPersonRoleMaintenanceDocument-parentBean"/>
    <bean id="ProtocolPersonRoleMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.kra.irb.personnel.ProtocolPersonRole"/>
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
        <property name="maintainableSections">
            <list>
                <ref bean="ProtocolPersonRoleMaintenanceDocument-EditProtocolPersonRole"/>
            </list>
        </property>
        <property name="lockingKeys">
            <list>
                <value>protocolPersonRoleId</value>
            </list>
        </property>
		<property name="allowsRecordDeletion" value="true" />
	
        
        <property name="documentTypeName" value="ProtocolPersonRoleMaintenanceDocument"/>
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
        
        
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="ProtocolPersonRoleMaintenanceDocument-EditProtocolPersonRole" parent="ProtocolPersonRoleMaintenanceDocument-EditProtocolPersonRole-parentBean"/>
    <bean id="ProtocolPersonRoleMaintenanceDocument-EditProtocolPersonRole-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Protocol Correspondence Type"/>
        <property name="title" value="Edit Protocol Correspondence Type"/>
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="protocolPersonRoleId" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="unitDetailsRequired" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="affiliationDetailsRequired" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="trainingDetailsRequired" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="commentsDetailsRequired" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
                <bean parent="MaintainableFieldDefinition" p:name="active"/>
            </list>
        </property>
    </bean>
</beans>
