<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="IacucValidProtoSubTypeQualMaintenanceDocument" parent="IacucValidProtoSubTypeQualMaintenanceDocument-parentBean" />
	<bean id="IacucValidProtoSubTypeQualMaintenanceDocument-parentBean"	abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.iacuc.actions.submit.IacucValidProtoSubTypeQual"/>
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
        <property name="maintainableSections">
        	<list>
        		<ref bean="IacucValidProtoSubTypeQualMaintenanceDocument-EditProtocolSubTypeQual" />
        	</list>
        </property>
        <property name="lockingKeys">
        	<list>
        		<value>validProtoSubTypeQualId</value>
        	</list>
        </property>
        <property name="allowsRecordDeletion" value="true" />
        <property name="documentTypeName" value="IacucValidProtoSubTypeQualMaintenanceDocument"/>
        <property name="businessRulesClass" value="org.kuali.kra.iacuc.actions.submit.IacucValidProtoSubTypeQualMaintenanceDocumentRule"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
	</bean>
	
	<!-- Maintenance Section Definitions -->
	
	<bean id="IacucValidProtoSubTypeQualMaintenanceDocument-EditProtocolSubTypeQual" parent="IacucValidProtoSubTypeQualMaintenanceDocument-EditProtocolSubTypeQual-parentBean" />
	<bean id="IacucValidProtoSubTypeQualMaintenanceDocument-EditProtocolSubTypeQual-parentBean" abstract="true" parent="MaintainableSectionDefinition" >
		<property name="id" value="Edit IACUC Valid Proto Sub Type Qual" />
		<property name="title" value="Edit IACUC Valid Proto Sub Type Qual" />
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="submissionTypeCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="submissionTypeQualCode" p:required="true"/>
				
			</list>
		</property>
	</bean>
</beans>
	
