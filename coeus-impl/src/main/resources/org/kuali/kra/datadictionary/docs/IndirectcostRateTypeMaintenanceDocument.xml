<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="IndirectcostRateTypeMaintenanceDocument" parent="IndirectcostRateTypeMaintenanceDocument-parentBean" />
    <bean id="IndirectcostRateTypeMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.kra.institutionalproposal.IndirectcostRateType" />
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl" />
        <property name="maintainableSections" >
            <list>
                <ref bean="IndirectcostRateTypeMaintenanceDocument-EditIndirectcostRateType" />
            </list>
        </property>
        <property name="lockingKeys" >
            <list>
                <value>indirectcostRateTypeCode</value>
            </list>
        </property>
        <property name="allowsRecordDeletion" value="true" />

        <property name="documentTypeName" value="Indirectcost Rate Type Maintenance Document" />
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase" />

    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="IndirectcostRateTypeMaintenanceDocument-EditIndirectcostRateType" parent="IndirectcostRateTypeMaintenanceDocument-EditIndirectcostRateType-parentBean" />
    <bean id="IndirectcostRateTypeMaintenanceDocument-EditIndirectcostRateType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Indirectcost Rate Type" />
        <property name="title" value="Edit Indirectcost Rate Type" />
        <property name="maintainableItems" >
            <list>
                <bean parent="MaintainableFieldDefinition"
                    p:name="indirectcostRateTypeCode"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="description"
                    p:required="true" />
                <bean parent="MaintainableFieldDefinition"
                    p:name="versionNumber" />
            </list>
        </property>
    </bean>
</beans>
