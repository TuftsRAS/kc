<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="IacucPersonTrainingMaintenanceDocument" parent="IacucPersonTrainingMaintenanceDocument-parentBean"/>
	<bean id="IacucPersonTrainingMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.iacuc.IacucPersonTraining"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="IacucPersonTrainingMaintenanceDocument-EditIacucPersonTraining"/>
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>iacucPersonTrainingId</value>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="true" />
	
		
		<property name="documentTypeName" value="IacucPersonTrainingMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.kra.iacuc.auth.IacucPersonTrainingMaintenanceDocumentAuthorizer"/>
	    <property name="businessRulesClass" value="org.kuali.kra.iacuc.rules.IacucPersonTrainingMaintenanceDocumentRule"/>
		
		
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="IacucPersonTrainingMaintenanceDocument-EditIacucPersonTraining" parent="IacucPersonTrainingMaintenanceDocument-EditIacucPersonTraining-parentBean"/>
	<bean id="IacucPersonTrainingMaintenanceDocument-EditIacucPersonTraining-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit IACUC Person Training"/>
		<property name="title" value="Edit IACUC Person Training"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="personTrainingId" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="speciesCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="procedureCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
			</list>
		</property>
	</bean>
</beans>
