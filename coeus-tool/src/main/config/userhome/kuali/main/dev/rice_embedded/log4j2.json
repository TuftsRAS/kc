{
 "configuration": {
  "name": "coeus-tool-template",
  "ThresholdFilter": {
   "level": "debug"
  },
  "appenders": {
   "Console": {
    "name": "STDOUT",
    "JSONLayout": {
     "eventEol": true,
     "properties": true,
     "locationInfo": false
    }
   }
  },
  "loggers": {
   "root": {
    "level": "info",
    "AppenderRef": {
     "ref": "STDOUT"
    }
   }
  }
 }
}
