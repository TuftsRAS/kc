<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="LookupableSponsor" parent="LookupableSponsor-parentBean" />
  <bean id="LookupableSponsor-parentBean" parent="Sponsor-parentBean" abstract="true">
    <property name="businessObjectClass" value="org.kuali.coeus.common.framework.sponsor.SponsorHierarchyMaintenanceLookupable" />
    <property name="lookupDefinition" >
      <ref bean="LookupableSponsor-lookupDefinition" />
    </property>
    </bean>
  <bean id="LookupableSponsor-lookupDefinition" parent="LookupableSponsor-lookupDefinition-parentBean" />
  <bean id="LookupableSponsor-lookupDefinition-parentBean" parent="Sponsor-lookupDefinition-parentBean" abstract="true">
    <property name="lookupableID" value="sponsorHierarchyMaintenanceLookupable" />
  </bean>
</beans>
