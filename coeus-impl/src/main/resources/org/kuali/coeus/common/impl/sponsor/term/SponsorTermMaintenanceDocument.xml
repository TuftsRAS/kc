 <beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="SponsorTermMaintenanceDocument" parent="SponsorTermMaintenanceDocument-parentBean"/>
	<bean id="SponsorTermMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.coeus.common.framework.sponsor.term.SponsorTerm"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="SponsorTermMaintenanceDocument-EditSponsorTerm"/>
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>sponsorTermId</value>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="true" />
	
		
		
		
		
		
		<property name="documentTypeName" value="SponsorTermMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
		
		
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="SponsorTermMaintenanceDocument-EditSponsorTerm" parent="SponsorTermMaintenanceDocument-EditSponsorTerm-parentBean"/>
	<bean id="SponsorTermMaintenanceDocument-EditSponsorTerm-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Sponsor Term"/>
		<property name="title" value="Sponsor Term"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="sponsorTermId" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="sponsorTermCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="sponsorTermTypeCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
			</list>
		</property>
	</bean>
</beans>
