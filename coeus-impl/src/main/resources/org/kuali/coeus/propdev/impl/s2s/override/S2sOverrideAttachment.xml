<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="S2sOverrideAttachment" parent="S2sOverrideAttachment-parentBean" />
  <bean id="S2sOverrideAttachment-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.coeus.propdev.impl.s2s.override.S2sOverrideAttachment" />
		<property name="objectLabel" value="S2s Override Attachment" />
    <property name="attributes" >
      <list>
        <ref bean="S2sOverrideAttachment-attachmentId" />
        <ref bean="S2sOverrideAttachment-contentId" />
        <ref bean="S2sOverrideAttachment-contentType" />
        <ref bean="S2sOverrideAttachment-name" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->


  <bean id="S2sOverrideAttachment-attachmentId" parent="S2sOverrideAttachment-attachmentId-parentBean" />
  <bean id="S2sOverrideAttachment-attachmentId-parentBean" abstract="true" parent="AttributeReferenceDummy-genericSystemId"
        p:name="attachmentId" p:shortLabel="Id" p:summary="Id" p:label="Id" p:description="Id">
  </bean>

  <bean id="S2sOverrideAttachment-contentId" parent="S2sOverrideAttachment-contentId-parentBean" />
  <bean id="S2sOverrideAttachment-contentId-parentBean" abstract="true" parent="S2sAppAttachments-contentId"
        p:shortLabel="Content Id" p:summary="Content Id" p:label="Content Id" p:description="Content Id" p:required="true">
  </bean>

  <bean id="S2sOverrideAttachment-contentType" parent="S2sOverrideAttachment-contentType-parentBean" />
  <bean id="S2sOverrideAttachment-contentType-parentBean" abstract="true" parent="S2sAppAttachments-contentType"/>

  <bean id="S2sOverrideAttachment-name" parent="S2sOverrideAttachment-name-parentBean"/>
  <bean id="S2sOverrideAttachment-name-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="name" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="File Name" />
    <property name="shortLabel" value="File Name" />
    <property name="maxLength" value="150" />
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="validCharactersConstraint">
      <bean parent="AnyCharacterPatternConstraint">
        <property name="allowWhitespace" value="true"/>
      </bean>
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition" p:size="52"/>
    </property>
    <property name="controlField">
      <bean p:size="52" parent="Uif-TextControl"/>
    </property>
    <property name="summary" value="File Name" />
    <property name="description" value="File Name" />
  </bean>

</beans>
