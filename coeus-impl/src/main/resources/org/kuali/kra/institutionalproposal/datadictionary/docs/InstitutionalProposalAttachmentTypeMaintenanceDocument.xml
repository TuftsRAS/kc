<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="InstitutionalProposalAttachmentTypeMaintenanceDocument" parent="InstitutionalProposalAttachmentTypeMaintenanceDocument-parentBean"/>
    <bean id="InstitutionalProposalAttachmentTypeMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalAttachmentType"/>
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
        <property name="maintainableSections">
            <list>
                <ref bean="InstitutionalProposalAttachmentTypeMaintenanceDocument-EditInstitutionalProposalAttachmentType"/>
            </list>
        </property>
        <property name="lockingKeys">
            <list>
                <value>attachmentTypeCode</value>
            </list>
        </property>
		<property name="allowsRecordDeletion" value="true" />
	
        
        <property name="documentTypeName" value="InstitutionalProposalAttachmentTypeMaintenanceDocument"/>
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
        
        
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="InstitutionalProposalAttachmentTypeMaintenanceDocument-EditInstitutionalProposalAttachmentType" parent="InstitutionalProposalAttachmentTypeMaintenanceDocument-EditInstitutionalProposalAttachmentType-parentBean"/>
    <bean id="InstitutionalProposalAttachmentTypeMaintenanceDocument-EditInstitutionalProposalAttachmentType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit Attachment Type"/>
        <property name="title" value="Edit Attachment Type"/>
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="attachmentTypeCode" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="allowMultiple"/>
            </list>
        </property>
    </bean>
</beans>
