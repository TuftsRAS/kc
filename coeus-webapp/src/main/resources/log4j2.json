{
 "configuration": {
  "name": "coeus-webapp",
  "ThresholdFilter": {
   "level": "debug"
  },
  "appenders": {
   "Console": {
    "name": "STDOUT",
    "JSONLayout": {
     "eventEol": true,
     "properties": true,
     "locationInfo": false
    }
   }
  },
  "loggers": {
   "logger": [
    {
     "name": "org.kuali.rice.core.impl.config.property.JAXBConfigImpl",
     "level": "info",
     "additivity": "false",
     "AppenderRef": {
      "ref": "STDOUT"
     }
    },
    {
     "name": "org.kuali.rice.core.framework.config.property.AbstractBaseConfig",
     "level": "info",
     "additivity": "false",
     "AppenderRef": {
      "ref": "STDOUT"
     }
    },
    {
     "name": "org.kuali.rice.krad.data.provider.impl.ProviderBasedDataObjectService",
     "level": "error",
     "additivity": "false",
     "AppenderRef": {
      "ref": "STDOUT"
     }
    },
    {
     "name": "org.kuali.rice.krad.bo.ModuleConfiguration",
     "level": "error",
     "additivity": "false",
     "AppenderRef": {
      "ref": "STDOUT"
     }
    },
    {
     "name": "bitronix.tm.twopc.Preparer",
     "level": "error",
     "additivity": "false",
     "AppenderRef": {
      "ref": "STDOUT"
     }
    }
   ],
   "root": {
    "level": "warn",
    "AppenderRef": {
     "ref": "STDOUT"
    }
   }
  }
 }
}