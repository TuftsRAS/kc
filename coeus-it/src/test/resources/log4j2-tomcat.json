{
  "configuration": {
    "name": "coeus-it-tomcat",
    "ThresholdFilter": {
      "level": "debug"
    },
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d [%t] u:%X{user}/d:%X{docId}/t:%X{testName} %-5p %c - %m%n"
        }
      }
    },
    "loggers": {
      "logger": [
        {
          "name": "org.kuali.rice",
          "level": "info",
          "additivity": "false",
          "AppenderRef": {
            "ref": "STDOUT"
          }
        },
        {
          "name": "org.kuali.kra",
          "level": "info",
          "additivity": "false",
          "AppenderRef": {
            "ref": "STDOUT"
          }
        },
        {
          "name": "org.kuali.coeus",
          "level": "info",
          "additivity": "false",
          "AppenderRef": {
            "ref": "STDOUT"
          }
        },
        {
          "name": "org.kuali.rice.krad.uif.lifecycle",
          "level": "error",
          "additivity": "false",
          "AppenderRef": {
            "ref": "STDOUT"
          }
        },
        {
          "name": "org.apache.catalina.core.ContainerBase.[Catalina].[localhost]",
          "level": "info",
          "additivity": "false",
          "AppenderRef": {
            "ref": "STDOUT"
          }
        },
        {
          "name": "org.apache.catalina",
          "level": "info",
          "additivity": "false",
          "AppenderRef": {
            "ref": "STDOUT"
          }
        }
      ],
      "root": {
        "level": "warn",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}
