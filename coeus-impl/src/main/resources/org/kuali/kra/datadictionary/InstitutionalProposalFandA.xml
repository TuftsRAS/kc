<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="InstitutionalProposalFandA" parent="InstitutionalProposalFandA-parentBean" />
    <bean id="InstitutionalProposalFandA-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalFandA" />
		<property name="objectLabel" value="Institutional Proposal Fand A" />
        <property name="inquiryDefinition" >
            <ref bean="InstitutionalProposalFandA-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="InstitutionalProposalFandA-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="proposalFandARateId" />
        <property name="attributes" >
            <list>
                <ref bean="InstitutionalProposalFandA-proposalFandARateId"/>
                <ref bean="InstitutionalProposalFandA-activityTypeCode"/>
                <ref bean="InstitutionalProposalFandA-fiscalYear"/>
                <ref bean="InstitutionalProposalFandA-onOffCampusFlag"/>
                <ref bean="InstitutionalProposalFandA-rateClassCode"/>
                <ref bean="InstitutionalProposalFandA-rateTypeCode"/>
                <ref bean="InstitutionalProposalFandA-startDate"/>
                <ref bean="InstitutionalProposalFandA-instituteRate"/>
                <ref bean="InstitutionalProposalFandA-applicableRate"/>
                <ref bean="InstitutionalProposalFandA-versionNumber" />
                <ref bean="InstitutionalProposalFandA-amount" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="InstitutionalProposalFandA-proposalFandARateId" parent="InstitutionalProposalFandA-proposalFandARateId-parentBean" />
    <bean id="InstitutionalProposalFandA-proposalFandARateId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalFandARateId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal FNA Id" />
        <property name="shortLabel" value="Proposal FNA Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal FNA Id" />
        <property name="description" value="Proposal FNA Id" />
    </bean>

    <bean id="InstitutionalProposalFandA-amount" parent="InstitutionalProposalFandA-amount-parentBean" />
    <bean id="InstitutionalProposalFandA-amount-parentBean" abstract="true" parent="KraAttributeReferenceDummy-genericAmount" >
        <property name="name" value="amount" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Amount" />
        <property name="shortLabel" value="Amount" />
        <property name="maxLength" value="22" />
        <property name="summary" value="Amount" />
        <property name="description" value="Amount" />
    </bean>

    <bean id="InstitutionalProposalFandA-activityTypeCode" parent="InstitutionalProposalFandA-activityTypeCode-parentBean" />
    <bean id="InstitutionalProposalFandA-activityTypeCode-parentBean" abstract="true" parent="BudgetRate-activityTypeCode">
        <property name="required" value="true" />
    </bean>

    <bean id="InstitutionalProposalFandA-fiscalYear" parent="InstitutionalProposalFandA-fiscalYear-parentBean"/>
    <bean id="InstitutionalProposalFandA-fiscalYear-parentBean" abstract="true" parent="BudgetRate-fiscalYear-parentBean"/>

    <bean id="InstitutionalProposalFandA-onOffCampusFlag" parent="InstitutionalProposalFandA-onOffCampusFlag-parentBean"/>
    <bean id="InstitutionalProposalFandA-onOffCampusFlag-parentBean" abstract="true" parent="BudgetRate-onOffCampusFlag-parentBean">
        <property name="required" value="true" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="CheckboxControlDefinition" />
        </property>
    </bean>

    <bean id="InstitutionalProposalFandA-rateClassCode" parent="InstitutionalProposalFandA-rateClassCode-parentBean"/>
    <bean id="InstitutionalProposalFandA-rateClassCode-parentBean" abstract="true"  parent="BudgetRate-rateClassCode-parentBean"/>

    <bean id="InstitutionalProposalFandA-rateTypeCode" parent="InstitutionalProposalFandA-rateTypeCode-parentBean"/>
    <bean id="InstitutionalProposalFandA-rateTypeCode-parentBean" abstract="true" parent="BudgetRate-rateTypeCode-parentBean">
        <property name="control" >
            <bean parent="SelectControlDefinition" p:valuesFinderClass="org.kuali.kra.award.budget.AwardFnARatesValuesFinder" p:includeKeyInLabel="false"/>
        </property>
    </bean>

    <bean id="InstitutionalProposalFandA-startDate" parent="InstitutionalProposalFandA-startDate-parentBean"/>
    <bean id="InstitutionalProposalFandA-startDate-parentBean" abstract="true" parent="BudgetRate-startDate-parentBean"/>

    <bean id="InstitutionalProposalFandA-instituteRate" parent="InstitutionalProposalFandA-instituteRate-parentBean"/>
    <bean id="InstitutionalProposalFandA-instituteRate-parentBean" abstract="true" parent="BudgetRate-instituteRate-parentBean"/>

    <bean id="InstitutionalProposalFandA-applicableRate" parent="InstitutionalProposalFandA-applicableRate-parentBean"/>
    <bean id="InstitutionalProposalFandA-applicableRate-parentBean" abstract="true" parent="BudgetRate-applicableRate" />

    <bean id="InstitutionalProposalFandA-versionNumber" parent="InstitutionalProposalFandA-versionNumber-parentBean" />
  <bean id="InstitutionalProposalFandA-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>
    <!-- Business Object Inquiry Definition -->
    <bean id="InstitutionalProposalFandA-inquiryDefinition" parent="InstitutionalProposalFandA-inquiryDefinition-parentBean" />
  <bean id="InstitutionalProposalFandA-InquiryView" parent="InstitutionalProposalFandA-InquiryView-parentBean"/>
    <bean id="InstitutionalProposalFandA-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Proposal FNA Rate Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="FNA Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="InstitutionalProposalFandA-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
    <property name="headerText" value="Proposal FNA Rate Id"/>
    <property name="dataObjectClassName" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalFandA"/>
    <property name="items">
      <list>
        <bean parent="Uif-Disclosure-GridSection">
          <property name="headerText" value="Proposal FNA Id"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
                <bean p:propertyName="proposalFandARateId" parent="Uif-DataField"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="InstitutionalProposalFandA-lookupDefinition" parent="InstitutionalProposalFandA-lookupDefinition-parentBean" />
  <bean id="InstitutionalProposalFandA-LookupView" parent="InstitutionalProposalFandA-LookupView-parentBean"/>
    <bean id="InstitutionalProposalFandA-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Proposal FNA Lookup" /><property name="defaultSort" >
      <bean parent="SortDefinition"/>
        </property>
        <property name="lookupFields" >
            <list>
        <bean parent="FieldDefinition" p:attributeName="proposalFandARateId" p:forceInquiry="true"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="proposalFandARateId" p:forceInquiry="true"/>
      </list>
    </property>
  </bean>

  <bean id="InstitutionalProposalFandA-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
    <property name="dataObjectClassName" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalFandA"/>
    <property name="headerText" value="Proposal FNA Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="proposalFandARateId" parent="Uif-LookupCriteriaInputField"/>
      </list>
    </property>
    <property name="resultFields" >
        <list>
            <bean p:propertyName="proposalFandARateId" parent="Uif-DataField"/>
        </list>
    </property>
    </bean>
</beans>
