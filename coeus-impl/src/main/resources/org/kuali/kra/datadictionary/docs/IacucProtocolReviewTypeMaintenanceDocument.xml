<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="IacucProtocolReviewTypeMaintenanceDocument" parent="IacucProtocolReviewTypeMaintenanceDocument-parentBean"/>
    <bean id="IacucProtocolReviewTypeMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
        <property name="businessObjectClass" value="org.kuali.kra.iacuc.actions.submit.IacucProtocolReviewType"/>
        <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
        <property name="maintainableSections">
            <list>
                <ref bean="IacucProtocolReviewTypeMaintenanceDocument-EditProtocolReviewType"/>
            </list>
        </property>
        <property name="lockingKeys">
            <list>
                <value>reviewTypeCode</value>
            </list>
        </property>
		<property name="allowsRecordDeletion" value="true" />
		<property name="allowsNewOrCopy" value="true" />
        <property name="documentTypeName" value="IacucProtocolReviewTypeMaintenanceDocument"/>
        <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    </bean>

    <!-- Maintenance Section Definitions -->
    
    <bean id="IacucProtocolReviewTypeMaintenanceDocument-EditProtocolReviewType" parent="IacucProtocolReviewTypeMaintenanceDocument-EditProtocolReviewType-parentBean"/>
    <bean id="IacucProtocolReviewTypeMaintenanceDocument-EditProtocolReviewType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
        <property name="id" value="Edit IACUC Protocol Review Type"/>
        <property name="title" value="Edit IACUC Protocol Review Type"/>
        <property name="maintainableItems">
            <list>
                <bean parent="MaintainableFieldDefinition" p:name="reviewTypeCode" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="globalFlag" p:required="true"/>
                <bean parent="MaintainableFieldDefinition" p:name="versionNumber"/>
            </list>
        </property>
    </bean>
</beans>
