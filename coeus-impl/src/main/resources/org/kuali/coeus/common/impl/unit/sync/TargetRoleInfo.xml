<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="TargetRole" parent="TargetRole-parentBean"/>
    <bean id="TargetRole-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.common.framework.unit.sync.TargetRoleInfo"/>
        <property name="objectLabel" value="Target Role"/>
        <property name="inquiryDefinition">
            <ref bean="TargetRole-inquiryDefinition"/>
        </property>
        <property name="lookupDefinition">
            <ref bean="TargetRole-lookupDefinition"/>
        </property>
        <property name="titleAttribute" value="id"/>
        <property name="attributes">
            <list>
                <ref bean="TargetRole-id"/>
                <ref bean="TargetRole-unitRoleSyncId"/>
                <ref bean="TargetRole-roleId"/>
                <ref bean="TargetRole-descends"/>
            </list>
        </property>
        <property name="relationships">
            <list>
                <bean parent="RelationshipDefinition">
                    <property name="objectAttributeName" value="role"/>
                    <property name="primitiveAttributes">
                        <list>
                            <bean parent="PrimitiveAttributeDefinition" p:sourceName="roleId" p:targetName="id"/>
                        </list>
                    </property>
                    <property name="supportAttributes">
                        <list>
                            <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="role.namespaceCode"
                                  p:targetName="namespaceCode"/>
                            <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="role.name" p:targetName="name"/>
                            <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="role.description" p:targetName="description"/>
                            <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="role.kimTypeId" p:targetName="kimTypeId"/>
                            <bean parent="SupportAttributeDefinition" p:identifier="true" p:sourceName="role.active" p:targetName="active"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->


    <bean id="TargetRole-id" parent="TargetRole-id-parentBean"/>
    <bean id="TargetRole-id-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="id"/>
        <property name="forceUppercase" value="false"/>
        <property name="label" value="Id"/>
        <property name="shortLabel" value="Id"/>
        <property name="maxLength" value="12"/>
        <property name="validationPattern">
            <bean parent="NumericValidationPattern"/>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition"
                  p:size="12"/>
        </property>
        <property name="summary" value="Id"/>
        <property name="description" value="Id"/>
    </bean>

    <bean id="TargetRole-unitRoleSyncId" parent="TargetRole-unitRoleSyncId-parentBean"/>
    <bean id="TargetRole-unitRoleSyncId-parentBean" abstract="true" parent="UnitRoleSync-id">
        <property name="name" value="unitRoleSyncId"/>
        <property name="label" value="Unit Role Sync Id"/>
        <property name="shortLabel" value="Unit Role Sync Id"/>
        <property name="summary" value="Unit Role Sync Id"/>
        <property name="description" value="Unit Role Sync Id"/>
    </bean>

    <bean id="TargetRole-roleId" parent="TargetRole-roleId-parentBean"/>
    <bean id="TargetRole-roleId-parentBean" abstract="true" parent="UnitRole-id">
        <property name="name" value="roleId"/>
        <property name="required" value="false"/>
        <property name="label" value="Role Id"/>
        <property name="shortLabel" value="Role Id"/>
        <property name="summary" value="Role Id"/>
        <property name="description" value="Role Id"/>
    </bean>

    <bean id="TargetRole-descends" parent="TargetRole-descends-parentBean"/>
    <bean id="TargetRole-descends-parentBean" abstract="true" parent="AttributeReferenceDummy-genericBoolean">
        <property name="name" value="descends"/>
        <property name="required" value="false"/>
        <property name="label" value="Descends"/>
        <property name="shortLabel" value="Descends"/>
        <property name="summary" value="Descends"/>
        <property name="description" value="Descends"/>
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="TargetRole-inquiryDefinition" parent="TargetRole-inquiryDefinition-parentBean"/>
    <bean id="TargetRole-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Target Role"/>
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Target Role"/>
                    <property name="numberOfColumns" value="1"/>
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
                            <bean parent="FieldDefinition" p:attributeName="unitRoleSyncId" p:forceInquiry="true"/>
                            <bean parent="FieldDefinition" p:attributeName="roleId" p:forceInquiry="true"/>
                            <bean parent="FieldDefinition" p:attributeName="descends"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>


    <!-- Business Object Lookup Definition -->
    <bean id="TargetRole-lookupDefinition" parent="TargetRole-lookupDefinition-parentBean"/>
    <bean id="TargetRole-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Target Role"/>
        <property name="defaultSort">
            <bean parent="SortDefinition"/>
        </property>
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id"/>
                <bean parent="FieldDefinition" p:attributeName="unitRoleSyncId"/>
                <bean parent="FieldDefinition" p:attributeName="roleId"/>
                <bean parent="FieldDefinition" p:attributeName="descends"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
                <bean parent="FieldDefinition" p:attributeName="unitRoleSyncId" p:forceInquiry="true"/>
                <bean parent="FieldDefinition" p:attributeName="roleId" p:forceInquiry="true"/>
                <bean parent="FieldDefinition" p:attributeName="descends"/>
            </list>
        </property>
    </bean>

</beans>