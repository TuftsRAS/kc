<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="SubAwardReportTypeMaintenanceDocument" parent="SubAwardReportTypeMaintenanceDocument-parentBean"/>
	<bean id="SubAwardReportTypeMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
		<property name="businessObjectClass" value="org.kuali.kra.subaward.bo.SubAwardReportType"/>
		<property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
		<property name="maintainableSections">
			<list>
				<ref bean="SubAwardReportTypeMaintenanceDocument-EditSubAwardReportType"/>
			</list>
		</property>
		<property name="lockingKeys">
			<list>
				<value>subAwardReportTypeCode</value>
			</list>
		</property>
		<property name="allowsRecordDeletion" value="true" />
		
		<property name="documentTypeName" value="SubAwardReportTypeMaintenanceDocument"/>
		<property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
		
		
	</bean>

	<!-- Maintenance Section Definitions -->
	
	<bean id="SubAwardReportTypeMaintenanceDocument-EditSubAwardReportType" parent="SubAwardReportTypeMaintenanceDocument-EditSubAwardReportType-parentBean"/>
	<bean id="SubAwardReportTypeMaintenanceDocument-EditSubAwardReportType-parentBean" abstract="true" parent="MaintainableSectionDefinition">
		<property name="id" value="Edit Report Type"/>
		<property name="title" value="Edit Report Type"/>
		<property name="maintainableItems">
			<list>
				<bean parent="MaintainableFieldDefinition" p:name="subAwardReportTypeCode" p:required="true"/>
				<bean parent="MaintainableFieldDefinition" p:name="description" p:required="true"/>
			</list>
		</property>
	</bean>
</beans>
