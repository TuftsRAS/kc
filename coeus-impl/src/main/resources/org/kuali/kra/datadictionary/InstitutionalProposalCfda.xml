<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="InstitutionalProposalCfda" parent="InstitutionalProposalCfda-parentBean" />
    <bean id="InstitutionalProposalCfda-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.kra.institutionalproposal.home.InstitutionalProposalCfda" />
		<property name="objectLabel" value="Institutional Proposal CFDA" />
        <property name="titleAttribute" value="cfdaNumber" />
        <property name="attributes" >
            <list>
                <ref bean="InstitutionalProposalCfda-proposalCfdaId" />
                <ref bean="InstitutionalProposalCfda-proposalId" />
                <ref bean="InstitutionalProposalCfda-proposalNumber" />
                <ref bean="InstitutionalProposalCfda-sequenceNumber" />
                <ref bean="InstitutionalProposalCfda-cfdaNumber" />
                <ref bean="InstitutionalProposalCfda-cfdaDescription" />
                <ref bean="InstitutionalProposalCfda-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="InstitutionalProposalCfda-proposalCfdaId" parent="InstitutionalProposalCfda-proposalCfdaId-parentBean" />
    <bean id="InstitutionalProposalCfda-proposalCfdaId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalCfdaId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal CFDA Id" />
        <property name="shortLabel" value="Proposal CFDA Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal CFDA Id" />
        <property name="description" value="Proposal CFDA Id" />
    </bean>

    <bean id="InstitutionalProposalCfda-proposalId" parent="InstitutionalProposalCfda-proposalId-parentBean" />
    <bean id="InstitutionalProposalCfda-proposalId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Id" />
        <property name="shortLabel" value="Proposal Id" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal Id" />
        <property name="description" value="Proposal Id" />
    </bean>

    <bean id="InstitutionalProposalCfda-proposalNumber" parent="InstitutionalProposalCfda-proposalNumber-parentBean" />
    <bean id="InstitutionalProposalCfda-proposalNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="proposalNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Proposal Number" />
        <property name="shortLabel" value="Proposal Number" />
        <property name="maxLength" value="8" />
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="AlphaNumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Proposal Number" />
        <property name="description" value="Proposal Number" />
    </bean>

    <bean id="InstitutionalProposalCfda-sequenceNumber" parent="InstitutionalProposalCfda-sequenceNumber-parentBean" />
    <bean id="InstitutionalProposalCfda-sequenceNumber-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="sequenceNumber" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Sequence Number" />
        <property name="shortLabel" value="Sequence Number" />
        <property name="maxLength" value="22" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Sequence Number" />
        <property name="description" value="Sequence Number" />
    </bean>

    <bean id="InstitutionalProposalCfda-cfdaNumber" parent="InstitutionalProposalCfda-cfdaNumber-parentBean" />
    <bean id="InstitutionalProposalCfda-cfdaNumber-parentBean" abstract="true" parent="CFDA-cfdaNumber" />

    <bean id="InstitutionalProposalCfda-cfdaDescription" parent="InstitutionalProposalCfda-cfdaDescription-parentBean"/>
    <bean id="InstitutionalProposalCfda-cfdaDescription-parentBean" abstract="true" parent="CFDA-cfdaProgramTitleName">
        <property name="name" value="cfdaDescription" />
    </bean>

    <bean id="InstitutionalProposalCfda-versionNumber" parent="InstitutionalProposalCfda-versionNumber-parentBean" />
  <bean id="InstitutionalProposalCfda-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>

</beans>
