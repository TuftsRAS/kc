<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="SerializableUserSessionAttribute" parent="SerializableUserSessionAttribute-parentBean" />
    <bean id="SerializableUserSessionAttribute-parentBean" abstract="true" parent="SerializableSessionAttribute-parentBean"
          p:businessObjectClass="org.kuali.coeus.sys.impl.session.ser.SerializableUserSessionAttribute"
          p:objectLabel="Serializable User Session Attribute">
        <property name="lookupDefinition" >
            <ref bean="SerializableUserSessionAttribute-lookupDefinition" />
        </property>
    </bean>

    <bean id="SerializableUserSessionAttribute-lookupDefinition" parent="SerializableUserSessionAttribute-lookupDefinition-parentBean" />
    <bean id="SerializableUserSessionAttribute-lookupDefinition-parentBean" abstract="true" parent="SerializableSessionAttribute-lookupDefinition-parentBean">
        <property name="title" value="Serializable User Session Attribute" />
        <property name="lookupableID" value="serializableUserSessionLookupable" />
    </bean>

</beans>
