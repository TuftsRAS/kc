<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="SerializableKradForm" parent="SerializableKradForm-parentBean" />
    <bean id="SerializableKradForm-parentBean" abstract="true" parent="SerializableSessionAttribute-parentBean"
          p:businessObjectClass="org.kuali.coeus.sys.impl.session.ser.SerializableKradForm"
          p:objectLabel="Serializable KRAD Form">
        <property name="lookupDefinition" >
            <ref bean="SerializableKradForm-lookupDefinition" />
        </property>
    </bean>

    <bean id="SerializableKradForm-lookupDefinition" parent="SerializableKradForm-lookupDefinition-parentBean" />
    <bean id="SerializableKradForm-lookupDefinition-parentBean" abstract="true" parent="SerializableSessionAttribute-lookupDefinition-parentBean">
        <property name="title" value="Serializable KRAD Form" />
        <property name="lookupableID" value="serializableKradFormLookupable" />
    </bean>

</beans>
